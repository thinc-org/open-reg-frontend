{
  "name": "open-reg-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "codegen": "ng-swagger-gen",
    "e2e": "ng e2e",
    "format": "prettier --write \"src/**/*.ts\"",
    "build:gzip": "gzip-all \"dist/**/*.{css,js,html}\"",
    "build": "ng build --prod && yarn build:gzip",
    "now-build": "ng build --configuration=staging",
    "pre-commit": "yarn lint && yarn format"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.5",
    "@angular/cdk": "^8.1.4",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/forms": "~8.2.4",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@zeit/ng-deploy": "^0.3.0",
    "angular-webstorage-service": "^1.0.2",
    "file-saver": "^2.0.2",
    "gzip-all": "^1.0.0",
    "hammerjs": "^2.0.8",
    "ng-swagger-gen": "^1.8.1",
    "ng-zorro-antd": "^8.1.2",
    "ngx-markdown": "^8.2.1",
    "ngx-strongly-typed-forms": "^8.0.1",
    "rxjs": "^6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.2",
    "@angular/cli": "~8.3.2",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "husky": "^3.0.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "<3.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "engines": {
    "node": ">= 10.0"
  }
}
